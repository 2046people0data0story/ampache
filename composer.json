{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-ctype": ">=7.1",
        "ext-curl": ">=7.1.3",
        "ext-date": ">=7.1",
        "ext-dom": ">=1.0",
        "ext-gd": ">=7.1.3",
        "ext-gmp": ">=7.1.3",
        "ext-iconv": ">=7.1",
        "ext-libxml": ">=7.1",
        "ext-mbstring": ">=7.1",
        "ext-pcre": ">=7.1",
        "ext-simplexml": ">=7.1",
        "ext-spl": ">=7.1",
        "ext-xmlreader": ">=7.1",
        "ext-xmlwriter": ">=7.1",
        "lib-libxml": ">=2.7.0",
        "belphemur/laravel-gettext": ">=7.1",
        "bstien/laravel-tvdb": "1.0.0",
        "captcha-com/laravel-captcha": ">=4.0",
        "fideloper/proxy": ">=4.0",
        "laravel/framework": ">=5.7",
        "laravel/tinker": ">=1.0",
        "laravelcollective/html": "^5.7",
        "mikealmond/musicbrainz": "^0.2.2",
        "php-tmdb/laravel": "^1.1",
        "spatie/laravel-permission": ">=2.7"
    },
    "repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/bstien/laravel-tvdb"
		}
    ],
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\" : [
				"modules",
				"modules/catalog",
				"modules/catalog/dropbox",
				"modules/catalog/local",
				"modules/catalog/remote",
				"modules/catalog/soundcloud",
				"modules/catalog/subsonic",
				"modules/catalog/seafile",
				"modules/plugin/7Digital",
				"modules/plugin/Amazon",
				"modules/plugin/Bitly",
				"modules/plugin/CatalogFavorites",
			    "modules/plugin/ChartLyrics",
                "modules/plugin/Discogs",
				"modules/plugin/FacebookS",
				"modules/plugin/Flattr",
				"modules/plugin/Flickr",
				"modules/plugin/FriendsTimeline",
				"modules/plugin/GoogleAnalytics",
				"modules/plugin/GoogleMaps",
				"modules/plugin/GooglePlus"
			]
		},
        "psr-0": {
            "MusicBrainz": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
        
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
