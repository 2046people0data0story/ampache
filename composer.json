{
	"name" : "ampache/laravel",
	"description" : "Ampache built on the Laravel Framework.",
	"keywords" : [
		"framework",
		"laravel"
	],
	"license" : "MIT",
	"type" : "project",
	"require" : {
		"php": ">=7.1.3",
		"belphemur/laravel-gettext": "^7.1",
		"captcha-com/laravel-captcha": "4.*",
		"cmgmyr/messenger": "^2.16",
		"doctrine/inflector": "~1.0",
		"edw/horde-browser": "2.0.15",
		"fideloper/proxy": "~4.0",
		"guzzlehttp/guzzle": "^6.3",
		"illuminate/contracts": "5.6.*",
		"illuminate/support": "5.6.*",
		"kunalvarma05/dropbox-php-sdk": "^0.2.1",
		"laravel/framework": "5.6.*",
		"laravel/socialite": "^3.0",
		"laravelcollective/html": "^5.6",
		"mikealmond/musicbrainz": "^0.2.2",
		"paragonie/random_compat": "2.0.*",
		"pbxg33k/musicbrainz": "^0.3.0",
		"predis/predis": "^1.1",
		"spatie/laravel-permission": "^2.7",
		"stechstudio/laravel-php-cs-fixer": "^1.0",
		"tonyhhyip/laravel-sse": "^1.0",
		"tonyhhyip/sse": "^2.1",
		"vkr/getid3": "^2.1",
		"wikimedia/composer-merge-plugin": "^1.4"
	},
	   "extra" : {
        "merge-plugin" : {
            "include" : [
                "modules/catalog/*/composer.module.json",
                "modules/localplay/*/composer.module.json",
                "modules/plugins/*/composer.module.json"
            ],
            "recurse" : false,
            "replace" : false,
            "merge-extra" : false
        }
    },
	"require-dev" : {
		"filp/whoops" : "~2.0",
		"fzaninotto/faker" : "~1.4",
		"mockery/mockery" : "~1.0",
		"phpunit/phpunit" : "~6.2"
	},

    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/mptre/php-soundcloud"
        },
   {
        "type": "package",
        "package": {
            "name": "edw/horde-browser",
            "version": "2.0.15",
            "source": {
                "url": "https://github.com/wagnered/horde-browser.git",
                "type": "git",
                "reference": "master"
            }
        }
    }

    ],

	"autoload" : {
		"classmap" : [
			"database/seeds",
			"database/factories"
		],
		 "psr-0": {
            "MusicBrainz": "src/"
        },
          "files" : [ 
             "app/Services/AmpError.php"
        ],
	"psr-4" : {
		  "Modules\\" : [
                "modules",
                "modules/catalogs",
                "modules/catalogs/dropbox",
                "modules/catalogs/local",
                "modules/catalogs/remote",
                "modules/catalogs/soundcloud",
                "modules/catalogs/subsonic",
                "modules/catalogs/seafile"
            ],
			"App\\" : "app/",
			"Classes\\" : "app/Classes/",
			"Traits\\" : "app/Traits/"
  		}
	},
	"autoload-dev" : {
		"psr-4" :
		 {
			"Tests\\" : "tests/"
		}
	},
	"scripts" : {
		"post-root-package-install" : "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
		"post-create-project-cmd" : "@php artisan key:generate",
		"post-autoload-dump" : [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"php artisan package:discover"
		]
	},
	"config" : {
		"preferred-install" : "dist",
		"sort-packages" : true,
		"optimize-autoloader" : true
	}
}
